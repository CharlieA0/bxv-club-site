function trialFunction() {
	//Clean up output each run
	document.getElementById("t_check").innerHTML = "";
	document.getElementById("t_results").innerHTML = "";	

	//Get input
	var x = document.getElementById("t_numb").value;
	var factors = "";	

	//Check input is valid
	document.getElementById("t_check").innerHTML = "Waiting...";
	if (x < 2 || x > 1000000000 || x == "") {
		document.getElementById("t_check").innerHTML = "Invalid Input :(";
		return;
	}
	//factor input
	else {
		document.getElementById("t_check").innerHTML = "Prime";
		var max = Math.sqrt(x);

		//Troubleshooting
		factors = max;

		/*for (i = 2; i <= max; i++) {
			if (x%i == 0){
				document.getElementById("t_check").innerHTML = "Composite";
				factors = factors + i
				factors = factors + ", " + x/i + ", "
			}
		}*/
	}
	//write output
	document.getElementById("t_results").innerHTML = factors;
}
